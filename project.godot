"language": "GDScript",
"path": "res://util/MAStar.gd"
}, {
"base": "Node",
"class": "MapGenerator",
"language": "GDScript",
"path": "res://util/MapGenerator.gd"
}, {
"base": "Node",
"class": "MapGrid",
"language": "GDScript",
"path": "res://util/MapGrid.gd"
}, {
"base": "Node",
